<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Turkish Lira Symbol (₺) - centered and styled -->
    <!-- The symbol is designed to fit within the safe zone (66dp centered in 108dp) -->
    
    <!-- Main ₺ symbol in white -->
    <group
        android:translateX="30"
        android:translateY="24">
        
        <!-- Vertical stroke of ₺ -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M24,0 L24,60 L32,60 L32,0 Z"/>
        
        <!-- Top horizontal line -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M8,16 L40,8 L40,14 L8,22 Z"/>
        
        <!-- Bottom horizontal line -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M8,30 L40,22 L40,28 L8,36 Z"/>
        
        <!-- Curved part (stylized L shape) -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M24,45 C24,45 24,55 35,55 L48,55 L48,60 L32,60 C18,60 18,50 18,45 L18,42 L24,40 Z"/>
        
        <!-- Small upward arrow integrated (growth symbol) -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M42,5 L48,12 L45,12 L45,20 L39,20 L39,12 L36,12 Z"/>
    </group>
</vector>